        "doctrine/dbal": "^2.6",
        "symfony/property-access": "^4.0",
        "symfony/serializer": "^4.0"
    },
    "require-dev": {
        "doctrine/collections": "^1.5",
        "phpunit/phpunit": "~5.7",
        "friendsofphp/php-cs-fixer": "^2.3",
        "fzaninotto/faker": "^1.7",
        "satooshi/php-coveralls": "^2.0"
    }
